{
  "name": "crisis-assist-backend",
  "version": "1.0.0",
  "description": "Backend services for CrisisAssist multi-agent system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:setup": "npm run db:create && npm run db:migrate",
    "db:create": "node -e \"require('./src/database/setup').createDatabase()\"",
    "db:migrate": "node -e \"require('./src/database/setup').runMigrations()\"",
    "db:seed": "node -e \"require('./src/database/setup').seedDatabase()\""
  },
  "dependencies": {
    "@descope/node-sdk": "^1.7.14",
    "@genkit-ai/ai": "^0.5.0",
    "@genkit-ai/core": "^0.5.0",
    "@genkit-ai/flow": "^0.5.0",
    "@genkit-ai/googleai": "^0.5.0",
    "@google/generative-ai": "^0.17.1",
    "@slack/web-api": "^7.10.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "genkit": "^0.5.0",
    "googleapis": "^159.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "pg": "^8.16.3",
    "socket.io": "^4.7.5",
    "twilio": "^5.9.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "crisis-assist",
    "multi-agent",
    "descope",
    "emergency-response",
    "oauth",
    "security"
  ],
  "author": "Your Name",
  "license": "MIT",
  "types": "./dist/index.d.ts"
}
